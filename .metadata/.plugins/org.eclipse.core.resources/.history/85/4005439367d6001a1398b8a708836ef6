package tree;

public class Tree {
	Node root;
	
	void insert(int data) 
	{
		Node newNode = new Node();
		newNode.data = data;
		if(root == null)
		{
			root = newNode;
		}
		else
		{
			Node next = root;
			if(data > next.data)
			{
				next.right = newNode;
			}
			else
			{
				next.left = newNode;
			}
			while(next.left != null && next.right != null)
			{
				
			}
		}
	}
	
	public void insertNode(Node node, int value) {
		if (value < node.data) 
		{
			if (node.left != null) {
				insertNode(node.left, value);
			} else {
				System.out.println(" Inserted " + value + " to left of " + node.data);
				node.left = new Node(value);
			}
		} 
		else if (value > node.data) {
			if (node.right != null) {
				insertNode(node.right, value);
			} else {
				System.out.println("  Inserted " + value + " to right of " + node.data);
				node.right = new Node(value);
			}
		}
    }
}
